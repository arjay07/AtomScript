@Game = {

    beats: $(p1, p2){
    
        if(p1 == "rock"){

            if(p2 == "rock")return 3;
            else if(p2 == "paper")return 2;
            else if(p2 == "scissors")return 1;
    
        }else if(p1 == "paper"){
    
            if(p2 == "rock")return 1;
            else if(p2 == "paper")return 3;
            else if(p2 == "scissors")return 2;
    
        }else if(p1 == "scissors"){
    
            if(p2 == "rock")return 2;
            else if(p2 == "paper")return 1;
            else if(p2 == "scissors")return 3;
    
        }else{
    
            return 4;
    
        }
    
    }

}